# .bash_profile

export DOTFILES=$HOME/.dotfiles

PATH=$PATH:/sbin:/bin
PATH=/usr/sbin:/usr/bin:$PATH
PATH=/usr/local/sbin:/usr/local/bin:$PATH
PATH=$HOME/bin:$DOTFILES/sbin:$DOTFILES/bin:$PATH
PATH=$HOME/.local/bin:$PATH
export PATH

# load interactive config
[[ $PS1 && -f ~/.bashrc ]] && . ~/.bashrc
